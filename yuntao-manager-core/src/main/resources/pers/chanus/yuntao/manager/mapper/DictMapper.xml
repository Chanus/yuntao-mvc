  <delete id="deleteByPrimaryKey" parameterType="java.lang.Integer">
    delete sys_dict, sys_dict_item from sys_dict
    left join sys_dict_item on sys_dict.dict_code = sys_dict_item.dict_code
    <where>
      <choose>
        <when test="list == null or list.size() == 0">
          and 1 &lt;&gt; 1
        </when>
        <when test="list.size() == 1">
          and sys_dict.id = <foreach collection="list" item="id">#{id,jdbcType=INTEGER}</foreach>
        </when>
        <otherwise>
          and sys_dict.id in <foreach collection="list" item="id" open="(" separator="," close=")">#{id,jdbcType=INTEGER}</foreach>
        </otherwise>
      </choose>
    </where>
  </delete>
  
